{
	"env": {
		"browser": true,
		"es6": true,
		"node": true,
		"commonjs": true
	},
	"extends": ["eslint:recommended"],
	"parserOptions": {
		"ecmaFeatures": {
			"experimentalObjectRestSpread": true
		},
		"sourceType": "module"
	},
	"rules": {
		"indent": 0,
		"quotes": [
			"error",
			"double"
		],
		"semi": [
			"error",
			"always"
		],
		"no-console": 0,
		"no-undef": 0,
		"no-unused-vars": 0
	}
}
